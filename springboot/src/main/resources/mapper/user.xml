<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.study.springboot.mapper.UserMapper">
     
    <resultMap id="userMap" type="com.study.springboot.model.User">
         <id column="id" property="id"/>
         <result column="name" property="name"/>
         <result column="age" property="age"/>
         <result column="stunumber" property="stunumber"/>
    </resultMap>


    <select id="queryById" resultMap="userMap">
          SELECT id,name,age,stunumber FROM test WHERE id=#{id}
    </select>
</mapper>